{
  "name": "octoprint-menubar-status",
  "private": true,
  "version": "1.0.0",
  "description": "A menubar application to monitor an octoprint installation",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "start:dev": "concurrently \"npm run build:rollup:watch\" \"NODE_ENV=development electron .\"",
    "build:rollup": "rollup -c",
    "build:rollup:watch": "rollup -c -w",
    "prettier": "prettier --write src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jakob Westhoff <jakob@westhoffswelt.de> (http://westhoffswelt.de)",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "concurrently": "^5.2.0",
    "devtron": "^1.4.0",
    "electron": "^8.2.3",
    "electron-reload": "^1.5.0",
    "prettier": "^2.0.5",
    "rollup": "^2.7.2",
    "rollup-plugin-amd": "^4.0.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "svelte": "^3.21.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "electron-store": "^5.1.1",
    "esm": "^3.2.25",
    "lodash": "^4.17.15",
    "menubar": "^8.0.2",
    "milligram": "^1.3.0",
    "node-fetch": "^2.6.0",
    "normalize.css": "^8.0.1",
    "serialize-error": "^6.0.0",
    "sockjs-client": "^1.4.0",
    "spinkit": "^2.0.1",
    "uuid": "^7.0.3"
  }
}
